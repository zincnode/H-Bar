{
  "name": "h-bar",
  "displayName": "H-Bar",
  "description": "A status bar to show system HW/SW stats",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "system status",
    "status bar",
    "cpu load",
    "memory usage",
    "uptime",
    "docker",
    "monitor"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "icon": "images/icon-hydrogen.png",
  "repository": "git@github.com:zincnode/H-Bar.git",
  "publisher": "zincnode",
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "systeminformation": "^5.18.3"
  }
}
